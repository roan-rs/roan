pub struct Base64Encoder {
    alphabetChars: string,
    padChar: char,
}

impl Base64Encoder {
    pub fn new(chars: char[]?, pad: char?) -> anytype {
        let chars = chars then chars else "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/".chars();
        let pad = pad then pad else '=';

        return Base64Encoder {
            alphabetChars: chars,
            padChar: pad,
        };
    }
}