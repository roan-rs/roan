export fn print(msg: string, ...args: string) -> void {
    let parts = msg.split("{}");

    if parts.len() != args.len() + 1 {
        throw "Invalid number of arguments";
    }

    __print(msg, ...args);
}

export fn println(msg: string, ...args: string) -> void {
    print(msg, ...args);
    print("\n");
}