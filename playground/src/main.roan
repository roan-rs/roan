use { println } from "std::debug"
pub struct Person {
    name: string,
    age: int,
    occupation: string,
}

impl Person {
    pub fn new(name: string, age: int, occupation: string) -> anytype {
        return Person {
            name: name,
            age: age,
            occupation: occupation,
        };
    }
}

pub const person = Person::new("John Doe", 30, "Software Engineer");

pub fn main() -> int {
    println("Person: {}", person);

    return 0;
}

main();
