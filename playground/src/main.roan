use { String } from "std::string"
use { println } from "std::debug"

fn print(text: string, ...args: anytype) -> void {
    println(text, ...args);
}

pub fn main() -> int {
    let c = '5';

    if c.is_digit() {
        print("Is digit");
    } else {
        print("Is not digit");
    }

    return 0;
}

main();
