use { println } from "std::debug";
use { base64 } from "std::base64";

struct Person {
    age: int,
    name: string,
}

impl Person {
    fn static() -> Person {
        return Person {
            age: 0,
            name: ""
        }
    }
}

fn main() {
    let person = Person::static();
    person.name = "John Doe"; // unimplmeneted

    println("Person: {}", person);
}

main();