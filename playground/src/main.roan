use { assert, println } from "std::debug"
use { Base64Encoder } from "std::base64";

pub fn main() -> int {
    assert(1 == 1, "1 is not equal to 1");

    let encoder = Base64Encoder::new("ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/");

    println("{}", encoder);

    return 0;
}

main();
